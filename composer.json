{
    "name": "gicharu/yii2-upload-file",
    "description": "Yii2 upload file tools",
    "keywords": ["yii", "upload", "file"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/mdmsoft/yii2-upload-file/issues",
        "source": "https://github.com/mdmsoft/yii2-upload-file"
    },
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap": "@dev",
        "yiisoft/yii2-imagine": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "mdm\\upload\\": ""
        }
    },
    "extra": {
        "bootstrap": "mdm\\upload\\Bootstrap",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
